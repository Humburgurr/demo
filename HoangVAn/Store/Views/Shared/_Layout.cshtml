@using ToyStore.Models;
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title> @ViewBag.Title </title>
    <script src="~/Scripts/jquery.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.scrollUp.min.js"></script>
    <script src="~/Scripts/price-range.js"></script>
    <script src="~/Scripts/jquery.prettyPhoto.js"></script>
    <script src="~/Scripts/main.js"></script>
    <link rel="icon" href="~/Assets/Images/shop/favicon.ico" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/prettyPhoto.css" rel="stylesheet" />
    <link href="~/Content/price-range.css" rel="stylesheet" />
    <link href="~/Content/animate.css" rel="stylesheet" />
    <link href="~/Content/mainshop.css" rel="stylesheet" />
    <link href="~/Content/responsive.css" rel="stylesheet" />
    <link href="~/Content/owlcarousel/assets/owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet" />
    <script src="~/Content/owlcarousel/owl.carousel.js"></script>
    <script src="~/Content/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/Content/Swiper/swiper-bundle.min.js"></script>
    <link href="~/Content/Swiper/swiper-bundle.min.css" rel="stylesheet" />
    <style>
        .banner {
            width: 90%;
            height: 200px;
        }

        .img-detail {
            width: 25%;
        }
    </style>
</head>
<body>
    <div class="master-wrapper-page">
        <div class="search-box store-search-box" id="search-box">
            @using (Html.BeginForm("Search", "Store", FormMethod.Get, new { @class = "search-box form" }))
            {
                @Html.TextBox("searchString", null, new { placeholder = "Tìm kiếm sản phẩm", type = "text", @class = "search-box-text", @id = "small-searchterms" });
            }
        </div>
        <!--header-->
        <header id="header" class="header">
            <div class="header-lower">
                <div class="header-logo">
                    <a href="/">
                        <h1><img alt="The Men" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAUsAAACYCAMAAABatDuZAAAAflBMVEX///8AAAC4uLi+vr7z8/MRERG7u7vl5eX7+/v4+Pjo6Og5OTktLS3e3t5jY2OLi4tERESurq4nJydaWloxMTHX19fFxcUJCQnR0dGZmZkbGxvLy8vu7u5QUFB7e3skJCRr    "></h1>
                    </a>
                </div>
                <div class="header-menu">
                    <ul class="top-menu notmobile">
                        <li class="topic-items-news">
                            <a href="@Url.Action("Products", "Store")">
                                Tất cả SP
                            </a>
                        </li>
                        @Html.Action("DongSP", "Store")

                        <li class="topic-items-news">
                            <a href="" @Url.Action("Store", "DongsSanPham") ">
                                Tin Tức
                            </a>
                        </li>
                        <li class="menu-news-items" style="margin-bottom:0"><a href="#">Khuyến mại</a></li>
                    </ul>
                </div>
                <div class="header-links-wrapper">
                    <div class="search-icon" onclick="myFunction()"></div>
                    <div class="header-links">
                        @Html.Action("GiohangPartial", "GioHang")
                    </div>
                    <div class="login-icon">
                        @if (Session["Taikhoan"] != null)
                        {
                            <a href="@Url.Action("Information", "User")">
                                <img src="~/Assets/Images/shop/login.png" alt="login-icon"> @*@(User.Identity.IsAuthenticated ? HttpContext.Current.User.Identity.Name: "Tài khoản")*@
                            </a>
                        }
                        else
                        {
                            <a href="@Url.Action("DangNhap", "User")">
                                <img src="~/Assets/Images/shop/login.png" alt="login-icon">
                            </a>
                        }
                    </div>
                </div>
            </div>
        </header><!--/header-->
        <div class="bg-sg" id="bg-sg" onclick="closeBoxSearch()"></div>
        <script>
            var element1 = document.getElementById("search-box");
            var element2 = document.getElementById("bg-sg");

            function myFunction() {
                element1.style.display = "block";
                element2.style.display = "block";
            }
            function closeBoxSearch() {
                element1.style.display = "none";
                element2.style.display = "none";
            }
        </script>
        <section class="center-1">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="features_items">
                            <!--features_items-->
                            <div class="item-box-list">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section>
            <div class="footer_newletter">
                <div class="heading_newletter">Đăng ký nhận tin từ The Men</div>
                <p>Thông tin sản phẩm mới nhất và chương trình khuyến mãi</p>
                <div class="newsletter">
                    <div class="title">
                        <strong>Bản tin</strong>
                    </div>
                    <div class="newsletter-subscribe" id="newsletter-subscribe-block">
                        <div class="newsletter-email">
                            <input id="newsletter-email" class="newsletter-subscribe-text" placeholder="Email của bạn" aria-label="Đăng ký nhận bản tin của chúng tôi" type="email" name="NewsletterEmail">
                            <button type="button" id="newsletter-subscribe-button" class="button-1 newsletter-subscribe-button">Đăng ký</button>
                        </div>
                        <div class="newsletter-validation">
                            <span id="subscribe-loading-progress" style="display:none" class="please-wait">Chờ đợi...</span>
                            <span class="field-validation-valid" data-valmsg-for="NewsletterEmail" data-valmsg-replace="true"></span>
                        </div>
                    </div>
                    <div class="newsletter-result" id="newsletter-result-block"></div>
                </div>
            </div>
        </section>
